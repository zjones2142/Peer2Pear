  <property name="styleSheet">
   <string notr="true">/* ── Base ── */
QMainWindow, QWidget#centralwidget, QWidget#appWidget { background-color: #0a0a0a; color: #f0f0f0; font-family: &quot;Segoe UI&quot;, &quot;SF Pro Text&quot;, Arial, sans-serif; }

/* ── Top Header ── */
QWidget#topHeader { background-color: #0f0f0f; border-bottom: 1px solid #1c1c1c; }
QLabel#logoLabel { color: #ffffff; font-size: 18px; font-weight: bold; padding-left: 8px; }

/* ── Search bar ── */
QLineEdit#searchEdit_12 { background-color: #1a1a1a; color: #cccccc; border: 1px solid #2a2a2a; border-radius: 18px; padding: 7px 16px 7px 16px; font-size: 13px; selection-background-color: #3a9e48; }
QLineEdit#searchEdit_12:focus { border: 1px solid #3a9e48; color: #ffffff; }
QLineEdit#searchEdit_12::placeholder { color: #555555; }

/* ── Settings button ── */
QToolButton#settingsBtn_12 { background-color: transparent; border: none; color: #666666; font-size: 18px; padding: 4px; border-radius: 6px; }
QToolButton#settingsBtn_12:hover { color: #aaffb0; background-color: #1a1a1a; }

/* ── Sidebar ── */
QWidget#sidebarWidget { background-color: #0f0f0f; border-right: 1px solid #1c1c1c; }
QWidget#profileWidget { background-color: #0f0f0f; border-bottom: 1px solid #1c1c1c; }

/* ── Profile avatar ── */
QLabel#profileAvatarLabel { background-color: #2e8b3a; color: #ffffff; font-size: 15px; font-weight: bold; border-radius: 20px; min-width: 40px; max-width: 40px; min-height: 40px; max-height: 40px; }
QLabel#profileNameLabel { color: #f0f0f0; font-size: 13px; font-weight: bold; }
QLabel#profileHandleLabel { color: #555555; font-size: 11px; }

/* ── New chat button ── */
QPushButton#newChatBtn { background-color: #1a2e1c; color: #5dd868; border: 1px solid #2e5e30; border-radius: 8px; font-size: 12px; font-weight: bold; padding: 6px 12px; }
QPushButton#newChatBtn:hover { background-color: #223a24; border-color: #3a9e48; color: #7aee85; }

/* ── Chat list ── */
QListWidget#chatList { background-color: #0f0f0f; border: none; outline: none; font-size: 14px; color: #d0d0d0; }
QListWidget#chatList::item { padding: 14px 16px; border-bottom: 1px solid #161616; min-height: 20px; }
QListWidget#chatList::item:selected { background-color: #162818; color: #ffffff; border-left: 3px solid #3a9e48; padding-left: 13px; }
QListWidget#chatList::item:hover:!selected { background-color: #141414; }

/* ── Scrollbars ── */
QScrollBar:vertical { background-color: transparent; width: 6px; margin: 0; }
QScrollBar::handle:vertical { background-color: #2a2a2a; border-radius: 3px; min-height: 30px; }
QScrollBar::handle:vertical:hover { background-color: #3a9e48; }
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical { height: 0; }
QScrollBar:horizontal { height: 0; }

/* ── Content area ── */
QWidget#contentWidget { background-color: #0a0a0a; }
QWidget#chatHeader { background-color: #0d0d0d; border-bottom: 1px solid #1c1c1c; }
QLabel#chatTitleLabel { color: #ffffff; font-size: 16px; font-weight: bold; letter-spacing: 0.2px; }
QLabel#chatSubLabel { color: #3a9e48; font-size: 11px; font-weight: bold; letter-spacing: 0.5px; }
QWidget#contentContainer { background-color: #0a0a0a; }

/* ── Tabs ── */
QTabWidget#mainTabs { background-color: #0a0a0a; }
QTabWidget#mainTabs::pane { border: none; background-color: #0a0a0a; }
QTabBar::tab { background-color: transparent; color: #555555; font-size: 13px; padding: 10px 20px; border: none; border-bottom: 2px solid transparent; margin-right: 2px; font-weight: 500; }
QTabBar::tab:selected { color: #5dd868; border-bottom: 2px solid #3a9e48; }
QTabBar::tab:hover:!selected { color: #aaaaaa; }

/* ── Message scroll ── */
QScrollArea#messageScroll { background-color: #0a0a0a; border: none; }
QWidget#scrollAreaWidgetContents { background-color: #0a0a0a; }

/* ── Files ── */
QScrollArea#filesScroll { background-color: #0a0a0a; border: none; }
QWidget#filesScrollContents { background-color: #0a0a0a; }
QFrame#fileCard1, QFrame#fileCard2, QFrame#fileCard3 { background-color: #141414; border: 1px solid #222222; border-radius: 12px; }
QLabel#fileThumb1, QLabel#fileThumb2, QLabel#fileThumb3 { background-color: #1a1a1a; color: #444444; font-size: 36px; border-radius: 8px; min-height: 140px; }
QLabel#fileName1, QLabel#fileName2, QLabel#fileName3 { color: #eeeeee; font-size: 13px; font-weight: bold; }
QLabel#fileSize1, QLabel#fileSize2, QLabel#fileSize3 { color: #666666; font-size: 11px; }
QLabel#fileSender1, QLabel#fileSender2, QLabel#fileSender3 { color: #444444; font-size: 11px; }
QPushButton#dlBtn1, QPushButton#dlBtn2, QPushButton#dlBtn3 { background-color: #1e5c26; color: #5dd868; border: 1px solid #2e7a36; border-radius: 8px; font-size: 13px; font-weight: bold; padding: 8px; min-height: 36px; }
QPushButton#dlBtn1:hover, QPushButton#dlBtn2:hover, QPushButton#dlBtn3:hover { background-color: #266e2e; color: #ffffff; }

/* ── Input bar ── */
QWidget#inputBar { background-color: #0d0d0d; border-top: 1px solid #1c1c1c; }
QToolButton#attachBtn { background-color: transparent; border: 1px solid #252525; border-radius: 20px; color: #555555; font-size: 16px; min-width: 40px; max-width: 40px; min-height: 40px; max-height: 40px; }
QToolButton#attachBtn:hover { background-color: #1a1a1a; border-color: #3a3a3a; color: #aaaaaa; }
QLineEdit#messageInput { background-color: #161616; color: #f0f0f0; border: 1px solid #252525; border-radius: 22px; padding: 11px 20px; font-size: 14px; selection-background-color: #2e8b3a; }
QLineEdit#messageInput:focus { border: 1px solid #2e6e32; background-color: #181818; }
QPushButton#sendBtn { background-color: #2e8b3a; color: #ffffff; border: none; border-radius: 20px; font-size: 14px; font-weight: bold; min-width: 80px; max-width: 80px; min-height: 40px; max-height: 40px; letter-spacing: 0.3px; }
QPushButton#sendBtn:hover { background-color: #38a844; }
QPushButton#sendBtn:pressed { background-color: #226228; }

/* ── Edit / Add buttons ── */
QToolButton#editProfileBtn, QToolButton#editItemBtn { background-color: transparent; border: none; color: #555555; font-size: 16px; border-radius: 6px; }
QToolButton#editProfileBtn:hover, QToolButton#editItemBtn:hover { color: #5dd868; background-color: #1a2e1c; }</string>
